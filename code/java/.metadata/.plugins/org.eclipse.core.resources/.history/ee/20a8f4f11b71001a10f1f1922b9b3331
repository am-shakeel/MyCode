package DP;

public class PermuteAString {

	public static void main(String[] args) {
		String s = "abcdef" ; 
		
		topDown(s) ; 
		
	}
	
	public static String topDown(String s) {
		
		if(s.length() == 0 ) { // Base Case 
			return "" ; 
		}
		
		for( int i=0 ; i < s.length(); i++ ) {
			System.out.println( s.charAt(i) + topDown(s.substring(i+1) ) );
		}
		
		
		return s ; 
	}
	
}