package DP;

public class PermuteAString {

	public static void main(String[] args) {
		String s = "abcdef" ; 
		
		topDown(s) ; 
		
	}
	
	public static String topDown(String s) {
		
		if(s.length() == 0 ) {
			return "" ; 
		}
		
		char output = s.charAt(0) ; 
		System.out.println(output + topDown(s.substring(1)) );
	
		
		
		
		return s; 
	}
	
}