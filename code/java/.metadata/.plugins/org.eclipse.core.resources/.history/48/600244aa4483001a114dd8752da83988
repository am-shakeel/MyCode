package DP;

public class MinEditDistance {
	
	public static void main(String[] args) {
		char A[] = {'b', 'e', 'n', 'y', 'a', 'm'} ; 
		char B[] = {'e', 'p', 'h', 'r', 'e', 'm' } ; 
		
		
		int[][] solution = new int[A.length][B.length] ; 
	
		solution[0][0] = 0; 
		
		for(int i=1 ; i<A.length; i++) { // Deletions
			solution[0][i] += 1 ;  
		}
		for(int i=1 ; i<A.length; i++) { // Insertions 
			solution[i][0] += 1 ;  
		}
		
	}
	
	public static void bottomUp() {
		
	}
	
}
